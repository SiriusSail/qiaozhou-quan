
.cell {
    position: relative;
    padding: 20rpx 32rpx;
    font-size: 28rpx;
    line-height: 48rpx;
    background-color: #fdfffd;
    margin-bottom: 1PX;
}
.border {
  border-bottom: 1px solid #e8e8e8;
}
.cell-container {
  display: flex;
  flex-direction: row;
}

.cell-container-main-left {
  box-sizing: border-box;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  color: #333333;
  font-weight: 500;
}

.cell-container-main-right {
  flex: 1;
  color: #999999;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}
.arrow::after {
  content: '>';
  display: block;
  padding-left: 20rpx;
}