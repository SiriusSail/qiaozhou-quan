.remarks {
  margin-top: 10rpx;
  color: #999;
  font-size: 22px;
}

.product {
  font-size: 26px;
}

.overNum {
  font-size: 20rpx;
  margin-top: 12rpx;
}

.extra {
  text-align: right;
}

.foot {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  font-size: 24rpx;
  padding-right: 40rpx;
}

.product-menu {
  display: flex;
  z-index: 99;
  height: 100vh;
  width: 100vw;
  overflow: hidden;
}
.sroll-menu {
  position: relative;
  background-color: #fafafa;
  width: 170rpx;
  font-weight: 400;
  height: 100%;
  overflow-y: auto;
  overflow-x: hidden;
}
.sroll-content {
  flex: 1 1;
  height: 100%;
  background-color: #fdfffd;
}

.scroll-view {
  height: 100%;
}

.bottom {
  height: 200rpx;
  width: 100%;
}

.category-name {
  line-height: normal;
  height: 100%;
  width: 100%;
  overflow: hidden;
  display: flex;
  align-items: center;
}

.category {
  width: 100%;
  word-break: break-all;
  word-wrap: break-word;
  white-space: pre-wrap;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  line-height: 38rpx;
  font-size: 22rpx;
  text-align: center;
}

.anna-tabs-vertical {
  height: 100%;
  display: flex;
  font-size: 28rpx;
  border-radius: 16rpx;
  overflow: hidden;
}
.anna-tabs-vertical-sidebar {
  position: relative;
  background-color: #fafafa;
  width: 170rpx;
  color: #000;
  font-weight: 400;
}
.anna-tabs-vertical-sidebar-item {
  position: relative;
  height: 48PX;
  line-height: 48PX;
  padding: 0 24rpx;
  white-space: nowrap;
  cursor: pointer;
}
.category-name-hover::after {
  position: absolute;
  content: '';
  left: 0;
  top: 16PX;
  width: 4PX;
  height: 16PX;
  border-radius: 4PX;
  background-color: #1890ff;
}

.anna-card-cover {
  padding-left: 10rpx;
  padding-right: 10rpx;
}
.anna-card {
  position: relative;
  overflow: hidden;
  border-radius: 16rpx;
  background-color: #fdfffd;
  font-size: 28rpx;
  margin-bottom: 16rpx;
  color: black;
}

.anna-card-horizontal {
  display: flex;
}

.anna-card-content {
  padding: 0 10rpx;
  flex: 1;
}

.anna-tag {
  font-size: 20rpx;
  box-sizing: border-box;
  position: relative;
  display: inline-block;
  text-align: center;
  color: #333;
  background-color: #f6f6f6;
  border: 2rpx solid #f6f6f6;
  height: 44rpx;
  line-height: 40rpx;
  padding: 0 14rpx;
  border-radius: 8rpx;
  margin-right: 14rpx;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
  white-space: nowrap;
}

.anna-space-item {
  display: flex;
  align-items: center;
}

.anna-space-align-center {
  align-items: center;
}

.anna-space {
  display: inline-flex;
}
.anna-space-horizontal-small {
  margin-right: 16rpx;
}

.cart-icon {
  position: relative;
}


.shopping-cart {
  position: fixed;
  width: 100vw;
  bottom: 0;
  padding: 20rpx 30rpx 40rpx;
  background-color: #fff;
  z-index: 1010;
  left: 0;
}

.row {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.anna-btn {
  position: relative;
  box-sizing: border-box;
  display: inline-block;
  padding: 0 32rpx;
  background-color: #ededef;
  height: 64rpx;
  line-height: 60rpx;
  border: 1PX solid #ededef;
  border-radius: 64rpx;
  color: #1890ff;
  font-weight: 500;
  font-size: 28rpx;
  text-align: center;
  white-space: nowrap;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
  touch-action: manipulation;
  -webkit-appearance: none;
  transition: all 0.3s cubic-bezier(0.645, 0.045, 0.355, 1);
  margin: 0;
  outline: 0;
  cursor: pointer;
  overflow: unset;
  font-weight: 500;
  font-size: 28rpx;
}

.anna-btn-look-orange {
  color: #fff;
  background-color: #fa8c16 !important;
  border-color: #fa8c16 !important;
}


.cart-icon {
  position: relative;
}

.cart-tal {
  position: absolute;
  top: 4rpx;
  height: 24rpx;
  width: 24rpx;
  text-align: center;
  line-height: 24rpx;
  right: -4rpx;
  font-size: 16rpx;
  color: #fff;
  background-color: red;
  border-radius: 50%;
}